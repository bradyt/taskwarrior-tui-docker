client.key=/root/opt/var/taskd/client.key.pem
server.cert=/root/opt/var/taskd/server.cert.pem
server.key=/root/opt/var/taskd/server.key.pem
server.crl=/root/opt/var/taskd/server.crl.pem
ca.cert=/root/opt/var/taskd/ca.cert.pem
debug.tls=3
